{
    "parser": "babel-eslint",
    "env": {
        "browser": true,
        "es2021": true
    },
    "plugins": [
        "testcafe"
    ],
    "extends": "plugin:testcafe/recommended",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "no-mixed-spaces-and-tabs": "error",
        "no-global-assign": ["error", {"exceptions": ["Object"]}],
        "no-self-assign": ["error", {"props": true}],
        "no-unused-labels": "error",
        "indent": [
            "error",
            4
        ],
        "linebreak-style": [
            "error",
            "unix"
        ]
    }
}